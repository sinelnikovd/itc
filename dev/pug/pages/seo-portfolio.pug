extends ../base/page

block title
	title Поддержка сайтов

block content
	+b.top-portfolio._seo
		.container
			+e.wrap
				+e.A.nav._prev(href="#")
					+e.SPAN.nav-arrow
						i.icon.icon_prev_cw
					+e.SPAN.nav-txt Окна Строй
				+e.A.nav._next(href="#")
					+e.SPAN.nav-txt Окна Строй
					+e.SPAN.nav-arrow
						i.icon.icon_next_cw
				+e.title Шувалов
				+e.items
					+e.item
						+e.icon.icon._start
						+e.label Начало сотрудничества:
						+e.txt Конец 2013 года
					+e.item
						+e.icon.icon._seo-work
						+e.label Основные работы:
						+e.txt поисковое продвижение сайта, маркетинговый аудит, поддержка и развитие сайта

				+e.btn-wrap
					+btn.A._border_normal._color_orange.__btn(href="#") Смотреть процесс разработки

	+b.seo-task
		.container
			+e.side._left
				+e.title Задача 
				+e.txt Служба Яндекс.Рефераты предназначена для студентов и школьников, дизайнеров и журналистов
			+e.side._right
				+e.title Решение 
				+e.txt Нажав на кнопку «Написать реферат», вы лично создаете уникальный текст, причем именно от вашего нажатия на кнопку зависит, какой именно текст получится — таким образом.


	+b.work-case
